{
  "expo": {
    "name": "simple-sda-hymnal",
    "slug": "hymnal",
    "version": "1.0.0",
    "newArchEnabled": true,
    "web": {
      "favicon": "./assets/web/favicon.ico"
    },
    "scheme": "hymnal",
    "experiments": {
      "tsconfigPaths": true,
      "reactCompiler": true
    },
    "plugins": ["expo-sqlite", "expo-router", "expo-audio"],
    "orientation": "portrait",
    "icon": "./assets/ios/AppIcon~ios-marketing.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cev.hymnal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/play_store_512.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cev.hymnal"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4b0e5c37-992f-499f-a148-7f5ee25afd59"
      }
    }
  }
}
